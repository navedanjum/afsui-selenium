<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="ITDevCenterSanitySuite">
    <listeners>
        <listener class-name="com.company.app.extentreport.ExtentReporterNG"/>
    </listeners>
    <test name="AFSTestCases" preserve-order="true" group-by-instances="true">
        <classes>
            <class name="com.company.app.testcases.ITDevCenterPageTests">
                <methods>
                    <include name="verifyITDevCenterContentSectionTest" />
                    <include name="verifyNavigationUsingLinksAndButtonsTest" />
                    <include name="verifyVideoLinkTest" />
                    <include name="verifyListedJobs" />
                </methods>
            </class>
        </classes>
    </test>
</suite>